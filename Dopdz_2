#include <iostream>

using namespace std;

int main()
{
int n;
cin >> n;

int **P=new int*[n];
for(int i=0; i<n; i++)
{
P[i]=new int[n];
}
int t=n/2;
for(int i=0; i<t; i++)
{
for(int j=0; j<t; j++)
{
if (j>=i) P[i][j]=i+1;
else P[i][j]=j+1;
P[i][n-(j+1)]=P[i][j];
P[n-(i+1)][j]=P[i][j];
P[n-(i+1)][n-(j+1)]=P[i][j];
}
}



if(n%2 == 1)
{
for(int i=0; i<t; i++)
{
P[i][t]=i+1;
P[n-(i+1)][t]=P[i][t];
P[t][i]=P[i][t];
P[t][n-(i+1)]=P[i][t];
}
P[t][t]=t;
}

for(int k=0; k < 2; k++)
{
for(int i = 0; i <= n; i++)
{
for(int j = 0; j <= n; j++)
{
cout << P[i][j];
}
for(int j = 0; j <= n; j++)
{
cout << P[i][j];

}
cout << endl;
}
}
for(int i=0; i<n; i++)
{
delete [] P[i];
}
delete [] P;

return 0;
}
