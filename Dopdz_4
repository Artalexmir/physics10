#include <iostream>
using namespace std;
int M(int* A, int a, int b, int n){
int x=a;
int y=b;
int m=A[n/2];
int p;
if(x==n/2)
{
   p=A[a];
   for(int i=a; i<x; i++)
   {
       if(A[i]<A[i+1])
       {
           p=A[i+1];
       }
   }
   return p;
}
if(y==n/2)
{
   p=A[y];
   for(int i=y; i<b; i++)
   {
       if(A[i]>A[i+1])
       {
           p=A[i+1];
       }
   }
   return p;
}
while (x<y)
{
    while (A[x]<m)
    {
        x++;
    }
    while(A[y]>m)
    {
        y--;
    }
    if(x<y){
        p=A[x];
        A[x]=A[y];
        A[y]=p;
        x++;
        y--;
    }
}
if(x<(n/2))
    return M(A, y, b, n);
else
    return M(A, a, x, n);

}
int main(){
int n;
double s=0;
cin>>n;
int* A=new int[n];
for(int i=0; i<n; i++)

{
    cin>>A[i];
    s+=A[i];
}
s=s/n;
n--;
int m=M(A, 0, n, n);
if(s>m)
    s=s-m;
else
    s=m-s;
cout<<s;
delete [] A;
return 0;
}
